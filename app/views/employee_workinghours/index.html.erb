<p id="notice"><%= notice %></p>

<h1>Listing Employee Workinghours</h1>

<table>
  <thead>
    <tr>
      <th>Employee</th>
      <th>Working hour</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_workinghours.each do |employee_workinghour| %>
      <tr>
        <td><%= employee_workinghour.employee %></td>
        <td><%= employee_workinghour.working_hour %></td>
        <td><%= link_to 'Show', employee_workinghour %></td>
        <td><%= link_to 'Edit', edit_employee_workinghour_path(employee_workinghour) %></td>
        <td><%= link_to 'Destroy', employee_workinghour, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Employee workinghour', new_employee_workinghour_path %>
