<p id="notice"><%= notice %></p>

<h1>Listing Employee Vacations</h1>

<table>
  <thead>
    <tr>
      <th>Employee workinghour</th>
      <th>Datum</th>
      <th>Urlaub</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employee_vacations.each do |employee_vacation| %>
      <tr>
        <td><%= employee_vacation.employee_workinghour %></td>
        <td><%= employee_vacation.datum %></td>
        <td><%= employee_vacation.urlaub %></td>
        <td><%= link_to 'Show', employee_vacation %></td>
        <td><%= link_to 'Edit', edit_employee_vacation_path(employee_vacation) %></td>
        <td><%= link_to 'Destroy', employee_vacation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

